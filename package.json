{
  "name": "zxcvbn-typescript",
  "version": "5.0.0-beta-1",
  "description": "realistic password strength estimation, updated and ported to Typescript from Dan Wheeler's zxcvbn",
  "author": "Tony Richards",
  "license": "MIT",
  "main": "./src/index.ts",
  "repository": "trichards57/zxcvbn",
  "scripts": {
    "test": "jest",
    "build": "npm run build-lib && npm run build-dist",
    "build-lib": "tsc",
    "build-dist": "make-dir dist && browserify --standalone zxcvbn -t uglifyify lib/index.js > dist/zxcvbn.js",
    "lint": "eslint"
  },
  "devDependencies": {
    "@types/jest": "^26.0.14",
    "@typescript-eslint/eslint-plugin": "^4.4.0",
    "@typescript-eslint/parser": "^4.4.0",
    "browserify": "^16.5.2",
    "eslint": "^7.10.0",
    "eslint-config-prettier": "^6.12.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.1.4",
    "jest": "^26.5.2",
    "make-dir-cli": "^2.0.0",
    "prettier": "^2.1.2",
    "ts-jest": "^26.4.1",
    "typescript": "^4.0.3",
    "uglifyify": "^5.0.2"
  },
  "keywords": [
    "password",
    "passphrase",
    "security",
    "authentication",
    "strength",
    "meter",
    "quality",
    "estimation",
    "pattern",
    "cracking",
    "scoring",
    "entropy",
    "bruteforce"
  ]
}
